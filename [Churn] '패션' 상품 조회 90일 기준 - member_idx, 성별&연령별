  where
    TD_TIME_RANGE(fashion_view.time, TD_DATE_TRUNC('day',TD_TIME_ADD(TD_SCHEDULED_TIME(), '-97d','KST'),'KST'), TD_DATE_TRUNC('day',TD_TIME_ADD(TD_SCHEDULED_TIME(), '-7d','KST'),'KST'), 'KST')
),

chrn_new_cnt_6 as -- Left Excluding JOIN
(
  select 
    period_7.age_band,
    period_7.sex,
    count(distinct period_7.member_idx) as churn_n_6
  from
    period_7 left join period_6 on period_7.member_idx = period_6.member_idx
  where 
    period_6.member_idx is null
  group by 1, 2
),

chrn_new_cnt_5 as
(
  select 
    period_6.age_band,
    period_6.sex,
    count(distinct period_6.member_idx) as churn_n_5
  from
    period_6 left join period_5 on period_6.member_idx = period_5.member_idx
  where 
    period_5.member_idx is null
  group by 1, 2
),

chrn_new_cnt_4 as
