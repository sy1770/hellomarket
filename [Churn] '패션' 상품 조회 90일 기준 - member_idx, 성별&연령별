SELECT
  TD_TIME_FORMAT(TD_DATE_TRUNC('day', TD_SCHEDULED_TIME(),'KST'), 'yyyy-MM-dd', 'KST') as date_observation,
  period_1_cnt.age_band,
  period_1_cnt.sex,
  period_1_cnt.period_1_total as start_total,
  chrn_new_cnt_0.churn_n_0 as churn_n
FROM 
  period_1_cnt left join chrn_new_cnt_0 on chrn_new_cnt_0.age_band = period_1_cnt.age_band and chrn_new_cnt_0.sex = period_1_cnt.sex
