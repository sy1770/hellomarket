  group by 1, 2
),

period_0_cnt as
(
  select age_band, sex, count(distinct member_idx) as period_0_total
  from period_0
  group by 1, 2
),

period_1_cnt as
(
  select age_band, sex, count(distinct member_idx) as period_1_total
  from period_1
  group by 1, 2  
)
