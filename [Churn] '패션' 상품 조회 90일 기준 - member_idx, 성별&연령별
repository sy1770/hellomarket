SELECT
  TD_TIME_FORMAT(TD_DATE_TRUNC('day',TD_TIME_ADD(TD_SCHEDULED_TIME(), '-6d','KST'),'KST'), 'yyyy-MM-dd', 'KST') as date_observation,
  period_7_cnt.age_band,
  period_7_cnt.sex,
  period_7_cnt.period_7_total as start_total,
  chrn_new_cnt_6.churn_n_6 as churn_n
FROM period_7_cnt left join chrn_new_cnt_6 on chrn_new_cnt_6.age_band = period_7_cnt.age_band and chrn_new_cnt_6.sex = period_7_cnt.sex

union all

SELECT
  TD_TIME_FORMAT(TD_DATE_TRUNC('day',TD_TIME_ADD(TD_SCHEDULED_TIME(), '-5d','KST'),'KST'), 'yyyy-MM-dd', 'KST') as date_observation,
  period_6_cnt.age_band,
  period_6_cnt.sex,
  period_6_cnt.period_6_total as start_total,
  chrn_new_cnt_5.churn_n_5 as churn_n
FROM period_6_cnt left join chrn_new_cnt_5 on chrn_new_cnt_5.age_band = period_6_cnt.age_band and chrn_new_cnt_5.sex = period_6_cnt.sex
  
union all

SELECT
