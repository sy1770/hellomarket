select 
left(transaction_date,6) as date_month, 
item_category_id, 
count(*) as transactions, 
sum(item_price) as GMV

from transaction_item_tbl

where
transaction_date >= date_format(date_sub(curdate(), interval 49 month), '%Y%m01000000')
and transaction_date < date_format(curdate(), '%Y%m%d000000')

group by date_month, item_category_id
